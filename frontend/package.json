{
  "name": "mr-kg-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "docker:build": "docker build -t mr-kg-frontend .",
    "docker:build-dev": "docker build -f Dockerfile.dev -t mr-kg-frontend-dev .",
    "docker:run": "docker run -p 3000:80 mr-kg-frontend",
    "docker:run-dev": "docker run -p 5173:5173 -v $(pwd):/app -v /app/node_modules mr-kg-frontend-dev"
  },
  "dependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.0"
  }
}
